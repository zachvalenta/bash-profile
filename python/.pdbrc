# grab pprint
from pprint import pprint
pp = pprint.PrettyPrinter(indent=4)

# list src after step over
alias n n;;l

# list src after step out
alias r r;;l

# print obj
alias p pp %1

# print obj attr
alias i pp dir(%1)

# print obj in scope after step over
alias loc pp(locals())
alias l l;;loc
alias ll ll;;loc
