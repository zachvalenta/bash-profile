{
    "editor.detectIndentation": false,
    "editor.fontSize": 15,
    "editor.lineNumbers": "relative",
    "editor.wordWrap": "on",
    "files.exclude": {
        "node_modules": true,
        "venv": true
    },
    "[markdown]": {
        "editor.quickSuggestions": true
    },
    "window.restoreWindows": "one",
    "window.zoomLevel": 0,
    "workbench.colorTheme": "Noctis Obscuro",
    "workbench.iconTheme": "material-icon-theme"
}