			"- [ ] `viz.md`: ",
			"",
			"* za: ",
			"* toil: ",
			""
		]
	},
	"checkbox": {
		"prefix": "mch",
		"description": "checkbox",
		"body": [
			"- [ ] $1"
		]
	},
	"code": {
		"prefix": "mdc",
		"description": "code block",
		"body": [
			"```$1",
			"$2",
			"```"
		]
	},
