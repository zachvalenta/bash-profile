{
	"filetree": {
		"prefix": "mft",
		"description": "file tree",
		"body": [
            "```sh",
            "root_file.txt",
            "├── dir",
            "│   └── file_1.txt",
            "│   └── file_2.txt",
            "```",
            ""
        ]
    },
    "day": {
		"prefix": "mdd",
		"description": "day",
		"body": [
			"# $CURRENT_YEAR.$CURRENT_MONTH.$CURRENT_DATE",
			"",
            "> ",
            "",
			"- [ ] `music.md`: ",
