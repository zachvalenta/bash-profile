[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[user]
	name = zachvalenta
	email = zjv5002@gmail.com
[color]
	ui = auto
[alias]
	ai = add -p
	b = branch -v
	c = commit -v
	co = checkout
	d = diff
	ds = diff --staged
    ma = checkout master
	count = rev-list --count HEAD
	far = remote -v show
	conf = config --global --list
	nb = checkout -b
	l = log --graph -n 10 --oneline --all --decorate  # more recent, prettier
	la = log --graph --oneline --all --decorate
	s = status -s  # shorter output
	t = show -s --format=%ci  # timestamp of last commit
	u = reset HEAD
[color]
	ui = always
[push]
	default = simple
[core]
    pager = delta --plus-color="#012800" --minus-color="#340001" --theme='ansi-dark'
[interactive]
    diffFilter = delta --color-only
